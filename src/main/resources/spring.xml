<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">-->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:annotation-config />

    <bean id = "triangle" class = "com.example.demo.Triangle" scope="prototype">
        <property name = "type" value = "Equilateral"/>
        <property name = "sides" value = "3"/>
        <property name = "y" value = "true"/>
    </bean>
    <bean id = "circle" class="com.example.demo.Circle">
        <property name="radius" value="20"/>
    </bean>
<!--    <bean id = "triangle1" class = "com.example.demo.Triangle">
        <property name = "type" value = "Quadrilateral"/>
        <property name = "sides" value = "4"/>
        <property name = "y" value = "false"/>
    </bean>-->
</beans>